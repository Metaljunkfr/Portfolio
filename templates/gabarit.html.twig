<!doctype html>
<html lang="fr" xmlns:og="http://ogp.me/ns#">
    <head>
        {% block meta %}{% endblock %}
        <!--Meta-->
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="ie=edge">
        <meta name="author" content="Guillaume Georges">
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
        <link rel="stylesheet" type="text/css" href="{{ asset ("css/css/app.css") }}">
        <link rel="icon" type="image/png" href="{{ asset ("img/favicon.png") }}"/>
        <title>{% block title %}Guillaume GEORGES - Développeur Web | Portfolio{% endblock %}</title>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
        <meta property="og:title" content="Portfolio - Guillaume GEORGES, Développeur Web/Mobile junior" />
        <meta property="og:type" content="website" />
        <meta property="og:url" content="https://www.guillaumegeorges.fr" />
        <meta property="og:image" content="https://www.guillaumegeorges.fr/img/logo.png" />
        <meta property="og:description" content="Guillaume Georges : Conception et réalisation de sites web. Développement back-end, front-end et bases de données." />
        <meta property="fb:app_id"          content="2746709065585291" />
    </head>

    <body class="gradient" id="page">

    <!--Menu navigation fixe-->
    <header>
        {% include('inc/nav.html.twig') %}
    </header>
    <div class="flashdiv">
        {# Affiche les messages flash erreur ou succes #}
        {% for label, messages in app.flashes %}
            {% for message in messages %}
                <div class="flash flash-{{ label }}">
                    {{ message }}
                </div>
            {% endfor %}
        {% endfor %}
    </div>

    <!--Contenu en fonction de la route appelée-->
    <div class="container-fluid alignremontepage" id="content">
        <main class="contenu">{% block main %}{% endblock %}</main>
    </div>

    <!--Bas de page fixe-->
    <div class="container-fluid">
        <footer>
            {% include('inc/footer.html.twig') %}
        </footer>
    </div>

    <!--Scripts JS-->
    {% block javascripts %}{% endblock %}
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
    <script>
        var coll = document.getElementsByClassName("deroule");
        var i;
        for (i = 0; i < coll.length; i++) {
            coll[i].addEventListener("click", function() {
                this.classList.toggle("active");
                var content = this.nextElementSibling;
                if (content.style.display === "block") {
                    content.style.display = "none";
                } else {
                    content.style.display = "block";
                }
            });
        }
    </script>
    <script>
        window.onload = function() {
            $("#remontepage").click(function(){
                $("html, body").animate({scrollTop: 0},"slow");
            });
        };
    </script>
    <script>
        var coll = document.getElementsByClassName("collapsible");
        var i;
        for (i = 0; i < coll.length; i++) {
            coll[i].addEventListener("click", function() {
                this.classList.toggle("active");
                var content = this.nextElementSibling;
                if (content.style.display === "block") {
                    content.style.display = "none";
                } else {
                    content.style.display = "block";
                }
            });
        }
    </script>
    </body>
</html>



